{
  "name": "sessions",
  "version": "1.0.0",
  "description": "sessions",
  "author": "Renato Almeida <renato.almeida@frame3nline.com>",
  "private": true,
  "scripts": {
    "dev": "tmux -2 new-session -d -s nuxt \\; split-window -v \\; select-pane -t 0 \\; resize-pane -U 10 \\; split-window -h \\; send-keys \"npm run dev-server\" C-m \\; select-pane -t 0 \\; send-keys \"npm run dev-client\" C-m \\; select-pane -t 2 \\; send-keys \"node proxy.js\" C-m \\; attach-session -t nuxt",
    "dev-client": "cross-env PORT=3001 nuxt",
    "dev-server": "sequelize db:migrate --config server/config/config.json --migrations-path server/migrations && cross-env PORT=3002 nodemon --watch server --inspect server/index.js",
    "build": "nuxt build",
    "start": "node server/index.js"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^2.0.5",
    "animejs": "^2.2.0",
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "express": "^4.15.5",
    "file-saver": "^1.3.3",
    "flat": "^4.0.0",
    "hammerjs": "^2.0.8",
    "http-proxy": "^1.16.2",
    "interactjs": "^1.3.3",
    "intro.js": "^2.8.0-alpha.1",
    "nuxt": "^1.0.0",
    "nuxt-device-detect": "^1.1.0",
    "opentok": "^2.6.1",
    "opentok-layout-js": "^1.1.0",
    "pg": "^7.4.1",
    "redis": "^2.8.0",
    "sequelize": "^4.11.3",
    "sequelize-cli": "^4.0.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "source-map-support": "^0.5.0",
    "vue-the-mask": "^0.11.1",
    "vuelidate": "^0.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "cross-env": "^5.0.5",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^22.1.4",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "susy": "^3.0.1",
    "tmux": "^0.1.3",
    "vue-svg-loader": "^0.5.0",
    "worker-loader": "^1.1.1"
  }
}
